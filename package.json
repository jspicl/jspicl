{
  "name": "@jspicl/jspicl",
  "version": "3.0.0",
  "description": "A javascript to PICO-8 LUA converter",
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "module": "dist/jspicl.esm.js",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "codecoverage": "npm t -- --coverage=true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/jspicl.cjs.production.min.js",
      "limit": "35 KB"
    },
    {
      "path": "dist/jspicl.esm.js",
      "limit": "35 KB"
    }
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AgronKabashi/jspicl"
  },
  "keywords": [
    "pico-8",
    "javascript",
    "buildtool",
    "lua",
    "ast",
    "transpile",
    "transpiler"
  ],
  "author": "Agron Kabashi",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@size-limit/preset-small-lib": "^4.8.0",
    "@types/esprima": "^4.0.2",
    "babel-loader": "^8.2.1",
    "husky": "^4.3.0",
    "react-is": "^17.0.1",
    "size-limit": "^4.8.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "esprima": "^4.0.1",
    "trastpiler": "^1.0.2"
  },
  "engines": {
    "node": ">=15"
  }
}
